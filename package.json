{
  "name": "EAuthenticator",
  "productname": "EAuthenticator",
  "version": "0.0.1",
  "description": "An Electron Desktop app compatible with Google Authenticator",
  "main": "main.js",
  "dependencies": {
    "about-window": "^1.10.0",
    "chart.js": "^2.7.2",
    "electron": "^1.8.0",
    "js-chacha20": "^1.1.0",
    "jssha": "^2.3.1",
    "material-design-icons": "^3.0.1",
    "material-design-lite": "^1.3.0",
    "openpgp": "^2.5.10",
    "secure-random": "^1.1.1"
  },
  "devDependencies": {
    "electron-packager": "^9.1.0",
    "mocha": "^3.5.0",
    "text-encoding": "^0.6.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Nikolasel/EAuthenticator.git"
  },
  "license": "GPL-3.0",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=img/icon300x300.png --prune=true --out=release-builds",
    "package-win": "electron-packager . EAuthenticator --overwrite --asar=true --platform=win32 --arch=ia32 --icon=img/icon300x300.png --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"EAuthenticator\"",
    "package-linux": "electron-packager . Eauthenticator --overwrite --asar=true --platform=linux --arch=x64 --icon=img/icon300x300.png --prune=true --out=release-builds"
  }
}
